@import "../../../templates/activityListTemp/activityListTemp.wxss";
page{height:100%;}
.enter-page{flex:1;width:100%;}
.activityListTemp{margin-bottom:20rpx;}
.enterinfo{background:#fff;padding-left:5%;margin-top:20rpx;}
.footer{position:fixed;bottom:0;left:0;height:120rpx;background:#fff;width:100%;padding-top:30rpx;z-index:10;}
.footer button{width:690rpx;height:89rpx;line-height:89rpx;margin:0 auto;color:#fff;}
.enterinfo .row{border-bottom:2rpx solid #e5e5e5;padding:30rpx 0;padding-right:5%;color:#353535;}
.enterinfo .row .placeholder{color:#999;}
.enterinfo .row .text{float:right;}
.enterinfo .row .text image{width:16rpx;height:23rpx;margin-left:10rpx;}
