.enterpriseListcard{width:690rpx;margin:0 auto;border-radius:10rpx;overflow:hidden;
	padding-bottom:10rpx;margin-bottom:32rpx;box-shadow:0px 0px 16px #e6e6e6;position:relative;}
.enterprisecard-image-box{width:100%;height:100%;position:absolute;top:0;left:0;overflow:hidden;}
.enterprisecard-image{width:100%;}
.enterprisecard-block1{padding:35rpx 20rpx 35rpx;overflow:hidden;position:relative;
	z-index:1; }
.enterprisecard-block1-content{display: flex;overflow:hidden;flex-direction:row;align-items: center}
.enterprisecard-logo{width:84rpx;height:84rpx;border-radius:10rpx;margin-right:20rpx;float:left;
	display: flex;align-items: center;border:1rpx solid #eeeeee;position:relative;z-index:10;}
.enterprisecard-logo>image{width:84rpx;height:84rpx;border-radius:10rpx;}
.enterprisecard-block1-text{width:544rpx;float:left;display: flex;flex-direction:column;position:relative;
	z-index:10;top:-6rpx;}
.enterprisecard-block1-title{margin-bottom:4rpx;color:#fff;}
.enterprisecard-block1-title>text{}
.enterprisecard-authentication{color:#f19942;display:inline-block;margin-left:10rpx;}
.enterprisecard-authentication>image{width:22rpx;height:18rpx;margin-right:5rpx;}
.enterprisecard-tag-text{padding:4rpx 10rpx;border:1rpx solid #ccc;margin-right:12rpx;display:inline-block;
	color:#fff;border-radius:6rpx;}
.enterprisecard-block2{padding:16rpx 20rpx;color:#888;line-height:45rpx;}
.enterprisecard-block2-person{overflow:hidden;}
.enterprisecard-block2-mapicon{width:24rpx;height:24rpx;margin-right: 8rpx;vertical-align: middle;}
.enterprisecard-block2-address{white-space:nowrap; overflow:hidden;text-overflow:ellipsis;
	max-width:200rpx;display:inline-block;}
.tag-yellow{border:1rpx solid #d1a578;color:#d1a578;}
.tag-green{border:1rpx solid #72b580;color:#72b580;}
.addressWrap{display:flex;align-items: center;}