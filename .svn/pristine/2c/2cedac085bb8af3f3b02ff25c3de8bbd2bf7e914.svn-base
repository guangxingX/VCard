import {programemodule} from "../../../../../module/programe";

const programe = new programemodule
Page({

  /**
   * 页面的初始数据
   */
  data: {
      ImageTextItem:[],//图文的信息 必须有
      pid:'-1', //缓存projectId
      options:{},//缓存options参数
      type:`-1`,//缓存type值

  },
    onTapSave(e){
        console.log(e.detail.ImageTextItem);
        // console.log(this.data.ImageTextItem);
        console.log(1)

        var pages = getCurrentPages();
        var prevPage = pages[pages.length - 2];
        // prevPage.setData({
        //     usePlan: this.data.ImageTextItem
        // })
        this.setData({
            ImageTextItem:e.detail.ImageTextItem,
        })
        switch (this.data.type) {
            case '1':
                //资金使用
                console.log(prevPage)
                prevPage.setData({
                    usePlan: this.data.ImageTextItem
                })
                wx.navigateBack()
                break;
            case '2':
                //业务规划
                prevPage.setData({
                    businessplanning: this.data.ImageTextItem
                })
                wx.navigateBack()
                break;
        }


    },
    onTapView(){
        console.log(2)
    },
  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
      console.log(options)
      //设置标题
      if(options.name){
          wx.setNavigationBarTitle({
              title: `${options.name}`,
              icon:'none'
          })
      }

      this.setData({
          options,
          pid:options.pid,
          type:options.type,
      })

      programe.getfinancingInfo(this.data.pid).then(res=>{
          console.log(res);
            switch (options.type) {
                case '1':
                    //资金使用规划
                this.setData({
                  

                  ImageTextItem: res.financingInfo.usePlan||[]
                    })
                    break;
                case '2':
                    //业务规划
                    this.setData({
                      ImageTextItem: res.financingInfo.businessplanning||[]
                    })
                    break;
            }

            // this.setData({
            //     ImageTextItem:res.imageText
            // })
      })



  },

  /**
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function () {

  },

  /**
   * 生命周期函数--监听页面显示
   */
  onShow: function () {

  },

  /**
   * 生命周期函数--监听页面隐藏
   */
  onHide: function () {

  },

  /**
   * 生命周期函数--监听页面卸载
   */
  onUnload: function () {

  },

  /**
   * 页面相关事件处理函数--监听用户下拉动作
   */
  onPullDownRefresh: function () {

  },

  /**
   * 页面上拉触底事件的处理函数
   */
  onReachBottom: function () {

  },

  /**
   * 用户点击右上角分享
   */
  onShareAppMessage: function () {

  }
})
