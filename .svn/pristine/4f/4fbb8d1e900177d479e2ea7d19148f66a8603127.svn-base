<view class="bg-f re " bind:tap="onTapLogo">
    <view wx:if="{{!title}}" class="must-tag ab-tag">
        必填
    </view>
    <view class=" p-20">
        <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
            <!---->
            <image wx:if="{{isShowlogo}}" src="{{logo?logo:'/pages/financial/edit/image/logo.png'}}" style="width: 50px; height: 50px; display: block" />
            <view wx:else  class="enterprisecard-logo">
                <view  class="company-logo-text">{{logoText}}</view>
            </view>
        </view>
        <view class="weui-cell__bd ">
            <view>{{title?title:'公司名称'}}</view>

            <view style="font-size: 13px;color: #888888;">{{Intro?Intro:'公司标签信息'}}</view>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>
</view>
