// pages/financial/projectinfo/preView/index.js
Page({

  /**
   * 页面的初始数据
   */
  data: {
      files:null,
      title:null,
      text:null,
      tag:null,
  },

    onBack(){
        wx.navigateBack()
    },
  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
     let data = JSON.parse(options.data)
      this.setData({
          files:data.files,
          title:data.title,
          text:data.text,
          tag:data.tag,
      })
  },

  /**
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function () {

  },

  /**
   * 生命周期函数--监听页面显示
   */
  onShow: function () {

  },

  /**
   * 生命周期函数--监听页面隐藏
   */
  onHide: function () {

  },

  /**
   * 生命周期函数--监听页面卸载
   */
  onUnload: function () {

  },

  /**
   * 页面相关事件处理函数--监听用户下拉动作
   */
  onPullDownRefresh: function () {

  },

  /**
   * 页面上拉触底事件的处理函数
   */
  onReachBottom: function () {

  },

  /**
   * 用户点击右上角分享
   */
  onShareAppMessage: function () {

  }
})
