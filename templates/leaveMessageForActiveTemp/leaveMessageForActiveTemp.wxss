.message-page{flex:1;width:100%;}
.messageli{padding:30rpx;background:#fff;color:#353535;border-bottom:1rpx solid #e5e5e5;position:relative;}
.messageListTemplate-logobox{display:flex;}
.messageListTemplate-logo{width:84rpx;height:84rpx;border-radius:50%;}
.messageListTemplate-titleWrap{margin-left:20rpx;width:582rpx;}
.messageListTemplate-title{overflow:hidden;text-overflow:ellipsis;color:#3073e3;
  display:-webkit-box;-webkit-box-orient:vertical;-webkit-line-clamp:2;padding-right: 30rpx;}
.messageListTemplate-messagecontent{margin-top:20rpx;}

.messageListTemplate-leaveName{color:#888888;padding-top: 10rpx;}


.messageli .messagecontent .leavemessage{color:#3073e3;}
.messageListTemplate-content{margin-top:20rpx;margin-bottom:20rpx;line-height: 1.3;}
.messageli .messagecontent .contact>text{margin-left:10rpx;}
.messageli .point{width:20rpx;height:20rpx;border-radius:50%;background:#f12c20;
  position:absolute;top:22rpx;left:104rpx;}


.detailsfooter{height:144rpx;position:fixed;bottom:0;left:0;width:100%;background:#efeff4;}
.detailsfooter button{width:329rpx;height:86rpx;line-height:86rpx;border-radius:10rpx;margin-top:28rpx;}
.detailsfooter button:nth-child(1){margin-left:30rpx;}
.detailsfooter button:nth-child(2){margin-right:30rpx;}
.supplybox{position: relative; width:100%;background:#f7f7f7;padding:25rpx 30rpx;border-radius:20rpx;margin-top:28rpx;box-sizing: border-box;}

.supplybox .content{color:#888;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.messageType{position:absolute;right:23rpx;top:23rpx;}
.inline{display:inline-block !important;}
.titleWrap{display:flex;align-items: center;justify-content:space-between;}
.leaveName{padding:10rpx 0;}
.noContent{text-align: center; position: absolute;left:50%;top:40%;margin-left: -128rpx;margin-top: -97rpx;}
.noImage{width: 256rpx;height: 194rpx;}
.noTextWrap{padding-top: 15rpx;}
.noText{font-size: 34rpx;color:#888888;}

.temp-phone-ico{width: 24rpx;height: 24rpx;margin-right: 8rpx;}
.temp-mobile-text{font-size: 28rpx;color:#3073e3;}



.leaveMegForActive-title{font-size: 32rpx; margin-bottom:10rpx;font-weight: 900;}
.leaveMegForActive-mapIco{width: 54rpx;height: 54rpx;margin-left: 15rpx;vertical-align: middle;}
.leaveMegForActive-address{padding-bottom: 15rpx;}
.leaveMegForActive-contentWrap{display:flex;align-items: center;}
.leaveMegForActive-endtime{margin-left: 90rpx;}